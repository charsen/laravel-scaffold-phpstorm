  <template name="s.fields" value="fields:    &#10;    $FIELD$: { name: '$NAME$', type: $TYPE$, $OTHER$$NEXT$ desc: '$DESC$' }&#10;    $END$" description="初始化字段项，带一个字段配置" toReformat="true" toShortenFQNames="true">
